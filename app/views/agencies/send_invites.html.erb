<div id="nav-bar-box">
  <div class="header-container">Invite Temps</div>
</div>


<div id="blue-box">
      <div class="padder">
        
        <br />
<% form_tag('send_invites') do %>
<table align="center" style="width:430px" cellpadding="0" border="0" cellspacing="0" >
  <tr>
    <td colspan="2" >
      <div id="left-header-box" >
        <div class="header-container">Invitations Sent</div>
      </div>
    </td>
  </tr>
  <tr>
    <td colspan="2">&nbsp;</td>
  </tr>
  <tr>
    <td colspan="2" align="center">Any emails that were not sent will be be because they are already registered with clockoff.com.</td>
  </tr>
  <tr>
    <td colspan="2">&nbsp;</td>
  </tr>
  <tr>
    <td class="header dotted_right">Email</td><td class="header" align="center">Sent</td>
  </tr>
  <% for email in @list %>
    <tr>
      <td class="dotted_right"><%= email[0] %></td><td align="center"><%= email[1] %></td>
    </tr>
  <% end %>
  <tr><td>&nbsp;</td></tr>
</table>
<% end %>
</div>
</div>
<script type="text/javascript">

    var navbarbox = RUZEE.ShadedBorder.create({corner:8, border:1});
    navbarbox.render('nav-bar-box');
    var bluebox = RUZEE.ShadedBorder.create({corner:8, border:1});
    bluebox.render('blue-box');

</script>